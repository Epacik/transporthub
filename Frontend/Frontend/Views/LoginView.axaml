<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ap="using:Frontend.AttachedProperties"
             xmlns:vm="using:Frontend.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Frontend.Views.LoginView"
             ap:Navigation.Header="Login"
             x:DataType="vm:LoginViewModel">
    <StackPanel>
        <TextBlock>Welcome to Avalonia!</TextBlock>
        <Button Command="{Binding OpenStartupSettings}">Settings</Button>
    </StackPanel>
</UserControl>
