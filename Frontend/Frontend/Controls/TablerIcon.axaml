<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="using:Frontend.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Frontend.Controls.TablerIcon"
             x:Name="Self">
  <TextBlock FontFamily="../Assets/Icons/tabler/tabler-icons.ttf#tabler-icons"
             Text="{CompiledBinding Icon, Source={ResolveByName Self}, DataType={x:Type local:TablerIcon}}"
             FontSize="{CompiledBinding Size, Source={ResolveByName Self}, DataType={x:Type local:TablerIcon}}"/>
</UserControl>
